{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-mbstring": "*",
        "doctrine/dbal": "^3.4",
        "ernysans/laraworld": "^1.0",
        "filament/filament": "^v2.14.1",
        "filament/notifications": "^2.0",
        "filament/spatie-laravel-media-library-plugin": "^2.13",
        "filament/spatie-laravel-tags-plugin": "^2.10",
        "flowframe/laravel-trend": "^0.1.1",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.2",
        "invaders-xx/filament-kanban-board": "0.2.6",
        "jeffgreco13/filament-breezy": "v1.3.12",
        "jonquihote/filament-static-chart-widgets": "^1.0",
        "laravel/framework": "v9.0.0",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^2.0",
        "psr/simple-cache": "^2.0"
    },
    "require-dev": {
        "laravel/telescope": "^4.9",
        "barryvdh/laravel-debugbar": "^3.6",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "platform": {
            "ext-pcntl": "8.0",
            "ext-posix": "8.0"
        }
    }
}
